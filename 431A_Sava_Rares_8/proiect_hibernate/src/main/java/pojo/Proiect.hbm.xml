<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pojo.Proiect" table="proiect"
		catalog="proiect" optimistic-lock="version">
		<id name="idproiect" type="java.lang.Integer">
			<column name="idproiect" />
			<generator class="identity" />
		</id>
		<many-to-one name="cursuri" class="pojo.Cursuri"
			fetch="select">
			<column name="idcurs" />
		</many-to-one>
		<many-to-one name="studenti" class="pojo.Studenti"
			fetch="select">
			<column name="idstudent" />
		</many-to-one>
		<property name="dataPredare" type="date">
			<column name="Data predare" length="10" />
		</property>
		<property name="denumire" type="string">
			<column name="Denumire" length="45" />
		</property>
		<property name="sesiuneExaminare" type="string">
			<column name="Sesiune examinare" length="45" />
		</property>
		<property name="descriere" type="string">
			<column name="Descriere" length="1000" />
		</property>
	</class>
</hibernate-mapping>
