<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pojo.Cursuri" table="cursuri" catalog="proiect"
		optimistic-lock="version">
		<id name="idcurs" type="java.lang.Integer">
			<column name="idcurs" />
			<generator class="identity" />
		</id>
		<property name="denumire" type="string">
			<column name="denumire" length="100" />
		</property>
		<property name="numeProfesor" type="string">
			<column name="nume_profesor" length="45" />
		</property>
		<property name="sala" type="string">
			<column name="sala" length="45" />
		</property>
		<property name="metodaNotare" type="string">
			<column name="metoda_notare" length="1000" />
		</property>
		<property name="structura" type="string">
			<column name="structura" length="1000" />
		</property>
		<set name="consultaties" table="consultatie" inverse="true"
			lazy="true" fetch="select">
			<key>
				<column name="idcurs" />
			</key>
			<one-to-many class="pojo.Proiect" />
		</set>
	</class>
</hibernate-mapping>
