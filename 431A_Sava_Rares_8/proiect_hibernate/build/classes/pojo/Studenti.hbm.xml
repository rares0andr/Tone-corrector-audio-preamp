<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="pojo.Studenti" table="studenti" catalog="proiect"
		optimistic-lock="version">
		<id name="idstudent" type="java.lang.Integer">
			<column name="idstudent" />
			<generator class="identity" />
		</id>
		<property name="nume" type="string">
			<column name="nume" length="45" />
		</property>
		
		<property name="initialaTata" type="string">
			<column name="initiala_tata" length="2" />
		</property>
		
		<property name="prenume" type="string">
			<column name="prenume" length="45" />
		</property>
		
		<property name="dataNasterii" type="date">
			<column name="data_nasterii" length="10" not-null="false" />
		</property>
		
		<property name="cnp" type="string">
			<column name="cnp" length="13" />
		</property>
		
		<property name="adresa" type="string">
			<column name="adresa" length="500" />
		</property>
		
		<property name="facultate" type="string">
			<column name="facultate" length="100" />
		</property>
		
		<property name="cicluStudii" type="string">
			<column name="ciclu_studii" length="10" />
		</property>
		
		<property name="anStudii" type="string">
			<column name="an_studii" length="1" />
		</property>
		
		<set name="consultaties" table="consultatie" inverse="true"
			lazy="true" fetch="select">
			<key>
				<column name="idstudent" />
			</key>
			<one-to-many class="pojo.Proiect" />
		</set>
	</class>
</hibernate-mapping>
